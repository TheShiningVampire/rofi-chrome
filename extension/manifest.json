{
  "name": "rofi-chrome",
  "description": "Unified Rofi picker for Chrome",
  "version": "1.1.0",
  "manifest_version": 3,

  "background": { "service_worker": "bg.js" },
  "action": { "default_title": "rofi-chrome" },

  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "storage",
    "sessions",
    "downloads",
    "topSites",
    "webNavigation",
    "nativeMessaging",
    "notifications"
  ],

  "host_permissions": ["<all_urls>"],

  "commands": {
    "rofiUnified": {
      "description": "Unified Rofi picker for Chrome"
    }
  },

  "key": ""
}
